<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Domain\Ship\Entity\ShipTo" table="ship_to" schema="shop">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="name" column="name" length="255" />
        <field name="address" column="address" length="255" />
        <field name="city" column="city" length="255" />
        <field name="country" column="country" length="255" />
    </entity>
</doctrine-mapping>
